function NonTwoHandedWeapon(entity)
    entity = entity or context.Target
    result = Character(entity) & ~WieldingWeapon('Twohanded', false, false, entity)
    return ConditionResult(result.Result, {ConditionError("TwoHandedWeapon")})
end

function AlreadyHasHexWarriorEffect(entity, mainHand)
    entity = entity or context.Target
    weaponEntity = GetActiveWeapon(entity, mainHand)
    if weaponEntity.IsValid then
        result = Character(entity) & ~HasStatus('PACT_OF_THE_BLADE_HEX_WARRIOR', weaponEntity) & ~HasStatus('BOUND_WEAPON_HEX_WARRIOR', weaponEntity) & (WieldingWeapon('Ammunition', false, true, entity) | WieldingWeapon('Melee', false, true, entity))
        return ConditionResult(result.Result, {ConditionError("AlreadyHasHexWarriorEffect")})
    end
    return ConditionResult(false, {ConditionError("AlreadyHasHexWarriorEffect")})
end

